package javaprogram;
import java.util.*;
public class area_of_circle {

	public static void main(String[] args) {
	double radius;
	double area;
	Scanner circle=new Scanner(System.in);
	 	 //get radius
		
		System.out.printf("\nEnter Radius Value :");
		radius=circle.nextDouble();
		
		area=22/7.0*radius;
		
		System.out.printf("\nArea of Circle :%.2f",area);
	
	}

}
